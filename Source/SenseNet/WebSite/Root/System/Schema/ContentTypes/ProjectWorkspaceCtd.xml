			 handler="SenseNet.ContentRepository.Workspaces.Workspace" 
			 xmlns="http://schemas.sensenet.com/SenseNet/ContentRepository/ContentTypeDefinition">
  <DisplayName>Project workspace</DisplayName>
  <Description>A workspace for project-oriented cooperation</Description>
  <Icon>workspace-project</Icon>
